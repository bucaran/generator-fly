{
  "name": "<%= pluginName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "repository": "https://github.com/<%= githubUserName %>/<%= pluginName %>",
  "main": "index.js",
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords":[
    "fly",
    "fly-plugin",
    "<%= pluginSlugName %>"
  ],
  "scripts": {
    "test": "xo && <%= testCommand %>"
  },
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  "devDependencies": {<% if (testTool == 'tape') { %>
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",<% } else if (testTool == 'mocha') { %>
    "mocha": "^3.0.2",<% } else if (testTool == 'jasmine') { %>
    "jasmine": "^2.5.0",<% } else if (testTool == 'ava') { %>
    "ava": "^0.16.0",<% } %>
    "fly": "^2.0.5",
    "fly-clear": "^1.0.1",
    "xo": "*"
  },
  "engines": {
    "node": ">= 4.6"
  },
  "xo": {
    "semicolon": false,
    "rules": {
      "generator-star-spacing": 0,
      "capitalized-comments": 0,
      "require-yield": 0
    }
  }
}
