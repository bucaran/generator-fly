{
  "name": "generator-fly",
  "version": "4.0.0",
  "engines": {
    "node": ">= 0.12.0"
  },
  "description": "Fly plugin template generator",
  "license": "MIT",
  "main": "generators/app/index.js",
  "repository": "https://github.com/flyjs/generator-fly",
  "author": {
    "name": "Oleh Kuchuk",
    "email": "kuchuklehjs@gmail.com",
    "url": "https://github.com/drKraken"
  },
  "contributors": [
    {
      "name": "Jorge Bucaran",
      "email": "jbucaran@me.com"
    }
  ],
  "scripts": {
    "test": "xo && mocha",
    "setup": "npm i && npm test",
    "deploy": "npm test && git push origin master && npm publish"
  },
  "files": [
    "generators"
  ],
  "keywords": [
    "yeoman-generator",
    "fly",
    "plugin",
    "template"
  ],
  "dependencies": {
    "clor": "^1.0.2",
    "mkdirp": "^0.5.1",
    "normalize-url": "^1.3.0",
    "yeoman-generator": "^0.19.2",
    "yosay": "^1.0.4"
  },
  "devDependencies": {
    "mocha": "*",
    "xo": "^0.16.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "space": true,
    "ignore": [
      "generators/app/templates/**/*.js"
    ],
    "rules": {
      "no-loop-func": 0
    },
    "overrides": [
      {
        "files": "test/*.js",
        "envs": [
          "node",
          "mocha"
        ]
      }
    ]
  }
}
